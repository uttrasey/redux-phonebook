{
  "name": "redux-phonebook",
  "version": "1.0.0",
  "description": "Playing around with a simple phonebook application using React & Redux",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "postinstall": "webpack --config config/webpack.js",
    "webpack": "webpack --config config/webpack.js",
    "webpack-watch": "webpack --config config/webpack.js --watch --colors",
    "test": "BABEL_JEST_STAGE=0 jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uttrasey/redux-phonebook.git"
  },
  "author": "Matt Uttridge",
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.3",
    "redux": "^3.0.4",
    "react-redux": "^4.0.0",
    "react-bootstrap": "^0.28.1",
    "classnames": "^2.1.5",
    "express": "^4.12.3",
    "body-parser": "^1.14.1",
    "jfs": "^0.2.6"
  },
  "devDependencies": {
    "babel": "^5.6.16",
    "babel-core": "^5.6.16",
    "babel-loader": "^5.3.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "json-loader": "^0.5.3",
    "react-addons-test-utils": "^0.14.3",
    "redux-devtools": "^2.1.5",
    "request": "^2.65.0",
    "webpack": "^1.8.4",
    "jest-cli": "*",
    "babel-jest": "*"
},
"jest": {
   "scriptPreprocessor": "<rootDir>/preprocessor.js",
   "unmockedModulePathPatterns": [
     "<rootDir>/node_modules/react",
     "<rootDir>/node_modules/react-dom",
     "<rootDir>/node_modules/react-addons-test-utils",
     "<rootDir>/node_modules/react-addons-pure-render-mixin",
     "<rootDir>/node_modules/fbjs",
     "<rootDir>/node_modules/redux"
   ]
 }
}
